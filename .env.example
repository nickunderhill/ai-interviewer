# PostgreSQL Database Configuration
# CRITICAL: Change POSTGRES_PASSWORD before first use!
# Use a strong password: 32+ characters, alphanumeric + symbols
# Generate secure password: openssl rand -base64 32
POSTGRES_USER=ai_interviewer_user
POSTGRES_PASSWORD=CHANGE_ME_TO_SECURE_PASSWORD_32_CHARS_MIN
POSTGRES_DB=ai_interviewer_db

# Database Connection (for backend)
# NOTE: DB_PASSWORD should match POSTGRES_PASSWORD above
DB_HOST=postgres
DB_PORT=5432
DB_NAME=ai_interviewer_db
DB_USER=ai_interviewer_user
DB_PASSWORD=CHANGE_ME_TO_SECURE_PASSWORD_32_CHARS_MIN
